{
  "name": "l4n-query",
  "version": "0.0.0",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore  .",
    "test": "tap lib/**test.js"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "4.8.0",
    "eslint-plugin-prettier": "^2.3.1",
    "prettier": "^1.7.4",
    "tap": "^10.7.2"
  },
  "author": "Bonuspunkt",
  "license": "MIT",
  "dependencies": {
    "debug": "^3.1.0"
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bonuspunkt/l4n-query.git"
  },
  "bugs": {
    "url": "https://github.com/Bonuspunkt/l4n-query/issues"
  },
  "homepage": "https://github.com/Bonuspunkt/l4n-query#readme",
  "description": "",
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "mocha": true
    },
    "parserOptions": {
      "ecmaVersion": 2017,
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    },
    "extends": [
      "eslint:recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "vars": "all",
          "args": "none"
        }
      ],
      "prettier/prettier": [
        "error",
        {
          "printWidth": 100,
          "singleQuote": true,
          "tabWidth": 4,
          "trailingComma": "all"
        }
      ]
    }
  }
}
